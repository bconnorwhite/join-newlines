{
  "name": "join-newlines",
  "version": "2.0.0",
  "description": "Join an array of strings with newlines",
  "license": "MIT",
  "author": "Connor White",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bconnorwhite/join-newlines.git"
  },
  "keywords": [
    "join",
    "newlines",
    "newline",
    "line"
  ],
  "files": [
    ".build"
  ],
  "main": "./.build/index.js",
  "scripts": {
    "build": "tsc --project ./tsconfig.json --incremental",
    "test": "jest"
  },
  "devDependencies": {
    "@swc/core": "^1.3.22",
    "@swc/jest": "^0.2.24",
    "jest": "^29.3.1",
    "typescript": "^4.9.4"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "source/**/*.ts"
    ],
    "coverageProvider": "v8",
    "coverageReporters": [
      "text"
    ],
    "testRegex": "test/(.*).test.ts$",
    "transform": {
      "^.+\\.tsx?$": [
        "@swc/jest"
      ]
    }
  }
}
